  return 'å€‹äººäº‹æ¥­ä¸»';
}

/**
 * Phase 4: å€‹äººäº‹æ¥­ä¸»ã®è©³ç´°æƒ…å ±æŠ½å‡º
 */
function extractIndividualBusinessDetails(title, snippet, url) {
  const result = {
    tradeName: '',
    representative: '',
    confidence: 0
  };
  
  const fullText = title + ' ' + snippet;
  
  // å±‹å·æŠ½å‡ºãƒ‘ã‚¿ãƒ¼ãƒ³
  const tradeNamePatterns = [
    /([^\s]{2,15})(äº‹å‹™æ‰€|å·¥æˆ¿|ã‚¹ã‚¿ã‚¸ã‚ª|ãƒ‡ã‚¶ã‚¤ãƒ³|ä¼ç”»)/g,
    /([^\s]{2,15})(å•†åº—|ã‚¹ãƒˆã‚¢|ã‚µãƒ­ãƒ³|ã‚¯ãƒªãƒ‹ãƒƒã‚¯|ãƒ©ãƒœ)/g,
    /([^\s]{3,20})(ã‚¢ãƒˆãƒªã‚¨|ã‚®ãƒ£ãƒ©ãƒªãƒ¼|ã‚ªãƒ•ã‚£ã‚¹)/g
  ];
  
  // ä»£è¡¨è€…æŠ½å‡ºãƒ‘ã‚¿ãƒ¼ãƒ³
  const representativePatterns = [
    /ä»£è¡¨[:ï¼š]\s*([^\s]{2,8})/g,
    /é‹å–¶è€…[:ï¼š]\s*([^\s]{2,8})/g,
    /ä»£è¡¨å–ç· å½¹[:ï¼š]\s*([^\s]{2,8})/g,
    /ä¸»å®°[:ï¼š]\s*([^\s]{2,8})/g,
    /è¬›å¸«[:ï¼š]\s*([^\s]{2,8})/g
  ];
  
  // å±‹å·æŠ½å‡º
  for (const pattern of tradeNamePatterns) {
    const match = fullText.match(pattern);
    if (match) {
      result.tradeName = match[0];
      result.confidence += 30;
      break;
    }
  }
  
  // ä»£è¡¨è€…æŠ½å‡º
  for (const pattern of representativePatterns) {
    const match = fullText.match(pattern);
    if (match) {
      result.representative = match[1];
