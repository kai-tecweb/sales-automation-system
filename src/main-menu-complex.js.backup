/**
 * ãƒãƒƒãƒå‡¦ç†ã§ã‚­ãƒ¼ãƒ¯ãƒ¼ãƒ‰ã‚’å®Ÿè¡Œæ™‚é–“åˆ¶é™å†…ã§å‡¦ç†
 */
function executeBatchCompanySearch(searchKeywords, searchKey, engineId, companySheet, companyHeaders, maxKeywordsPerBatch) {
  const startTime = Date.now();
  const MAX_EXECUTION_TIME = 5.5 * 60 * 1000; // 5.5åˆ†ã‚’ãƒŸãƒªç§’ã«å¤‰æ›
  
  // ãƒãƒƒãƒå‡¦ç†çŠ¶æ…‹ã®ç¢ºèª
  const batchStatusKey = 'BATCH_SEARCH_STATUS';
  const batchStatus = JSON.parse(PropertiesService.getScriptProperties().getProperty(batchStatusKey) || '{"completed": 0, "total": 0}');
  
  let processedCount = batchStatus.completed || 0;
  
  if (processedCount === 0) {
    // æ–°è¦å®Ÿè¡Œ
    batchStatus.total = searchKeywords.length;
    PropertiesService.getScriptProperties().setProperty(batchStatusKey, JSON.stringify(batchStatus));
  }
  
  // æ®‹ã‚Šã®ã‚­ãƒ¼ãƒ¯ãƒ¼ãƒ‰ã‚’å–å¾—
  const remainingKeywords = searchKeywords.slice(processedCount);
  const currentBatchSize = Math.min(maxKeywordsPerBatch, remainingKeywords.length);
  const currentBatch = remainingKeywords.slice(0, currentBatchSize);
  
  if (currentBatch.length === 0) {
    // å…¨ã¦å®Œäº†æ¸ˆã¿
    PropertiesService.getScriptProperties().deleteProperty(batchStatusKey);
    return {
      message: 'å…¨ã¦ã®ã‚­ãƒ¼ãƒ¯ãƒ¼ãƒ‰å‡¦ç†ãŒå®Œäº†ã—ã¦ã„ã¾ã™ï¼',
      isComplete: true
    };
  }
  
  console.log(`ãƒãƒƒãƒå‡¦ç†: ${currentBatch.length}å€‹ã®ã‚­ãƒ¼ãƒ¯ãƒ¼ãƒ‰ã‚’å‡¦ç†ä¸­...`);
  
  // å®Ÿéš›ã®æ¤œç´¢å‡¦ç†ã‚’å®Ÿè¡Œ
  const searchResult = performCompanySearchBatch(currentBatch, searchKey, engineId);
  
  // çµæœã‚’ã‚·ãƒ¼ãƒˆã«æ›¸ãè¾¼ã¿
  if (searchResult.companies && searchResult.companies.length > 0) {
    const nextRow = companySheet.getLastRow() + 1;
    const companyData = searchResult.companies.map(company => [
      company.name || '',
      company.website || '',
      company.industry || '',
      company.size || '',
      company.location || '',
      company.description || '',
      company.score || 0,
      company.keyword || '',
      new Date(),
      '',
      '',
      company.businessType || '',
      company.tradeName || '',
      company.representative || '',
      company.confidence || 0,
      company.established || '',
      company.capital || ''
    ]);
    
    companySheet.getRange(nextRow, 1, companyData.length, companyHeaders.length).setValues(companyData);
  }
  
  // é€²æ—ã‚’æ›´æ–°
  processedCount += currentBatch.length;
  batchStatus.completed = processedCount;
  PropertiesService.getScriptProperties().setProperty(batchStatusKey, JSON.stringify(batchStatus));
  
  // å®Œäº†ãƒã‚§ãƒƒã‚¯
  const isComplete = processedCount >= searchKeywords.length;
  if (isComplete) {
    PropertiesService.getScriptProperties().deleteProperty(batchStatusKey);
    return {
      message: `å…¨ã¦ã®å‡¦ç†ãŒå®Œäº†ã—ã¾ã—ãŸï¼\n\nâ€¢ å‡¦ç†ã‚­ãƒ¼ãƒ¯ãƒ¼ãƒ‰: ${processedCount}å€‹\nâ€¢ ç™ºè¦‹ä¼æ¥­æ•°: ${searchResult.companies ? searchResult.companies.length : 0}ç¤¾`,
      isComplete: true
    };
  } else {
    const remaining = searchKeywords.length - processedCount;
    return {
      message: `ãƒãƒƒãƒå‡¦ç†å®Œäº†ï¼\n\nâ€¢ å‡¦ç†æ¸ˆã¿: ${processedCount}/${searchKeywords.length}å€‹\nâ€¢ ç™ºè¦‹ä¼æ¥­æ•°: ${searchResult.companies ? searchResult.companies.length : 0}ç¤¾\nâ€¢ æ®‹ã‚Š: ${remaining}å€‹\n\næ®‹ã‚Šã®ã‚­ãƒ¼ãƒ¯ãƒ¼ãƒ‰ãŒã‚ã‚Šã¾ã™ã€‚å†åº¦å®Ÿè¡Œã—ã¦ãã ã•ã„ã€‚`,
      isComplete: false
    };
  }
}

/**
 * å®Ÿéš›ã®ãƒãƒƒãƒä¼æ¥­æ¤œç´¢å‡¦ç†
 */
function performCompanySearchBatch(keywords, searchKey, engineId) {
  const companies = [];
  let processedCount = 0;
  
  for (const keyword of keywords) {
    try {
      SpreadsheetApp.getActiveSpreadsheet().toast(`ã€Œ${keyword}ã€ã§æ¤œç´¢ä¸­...`, 'ğŸ” ä¼æ¥­æ¤œç´¢', 3);
      
      // Google Custom Search APIå‘¼ã³å‡ºã—
      const searchResults = performGoogleSearchSimple(keyword, searchKey, engineId);
      
      if (searchResults && searchResults.items) {
        for (const item of searchResults.items) {
          // ä¼æ¥­æƒ…å ±ã‚’è§£æãƒ»ã‚¹ã‚³ã‚¢ãƒªãƒ³ã‚°
          const companyInfo = analyzeSearchResultSimple(item, keyword);
          if (companyInfo && companyInfo.score >= 25) { // é–¾å€¤ã‚’25ã«å¤§å¹…ç·©å’Œ
            // å³å¯†å¾Œå‡¦ç†ãƒ•ã‚£ãƒ«ã‚¿ãƒªãƒ³ã‚°é©ç”¨ï¼ˆç·©å’Œç‰ˆï¼‰
            // ãƒ‡ãƒãƒƒã‚°ç”¨ï¼šå¾Œå‡¦ç†ãƒ•ã‚£ãƒ«ã‚¿ã‚’ä¸€æ™‚çš„ã«ç·©ãã™ã‚‹
            const passesFilter = true; // strictPostProcessFilter(companyInfo); // ä¸€æ™‚çš„ã«å…¨ã¦é€šã™
            
            if (passesFilter) {
              // é‡è¤‡ãƒã‚§ãƒƒã‚¯
              const isDuplicate = companies.some(existing => 
                existing.website === companyInfo.website || existing.name === companyInfo.name
              );
              
              if (!isDuplicate) {
                companies.push(companyInfo);
              }
            }
          }
        }
      }
      
      processedCount++;
      
      // ãƒ¬ãƒ¼ãƒˆåˆ¶é™å¯¾ç­–ï¼ˆ2ç§’å¾…æ©Ÿï¼‰
      Utilities.sleep(2000);
      
      // é€²æ—æ›´æ–°
      SpreadsheetApp.getActiveSpreadsheet().toast(
        `é€²æ—: ${processedCount}/${keywords.length} (${Math.round((processedCount / keywords.length) * 100)}%)`, 
        'ğŸ” ä¼æ¥­æ¤œç´¢ä¸­', 
        2
      );
      
    } catch (error) {
      console.error(`ã‚­ãƒ¼ãƒ¯ãƒ¼ãƒ‰ã€Œ${keyword}ã€ã®æ¤œç´¢ã§ã‚¨ãƒ©ãƒ¼:`, error);
      continue;
    }
  }
  
  return { companies: companies, processed: processedCount };
}

/**
 * ã‚·ãƒ³ãƒ—ãƒ«ãªGoogleæ¤œç´¢å®Ÿè¡Œ
 */
function performGoogleSearchSimple(keyword, searchKey, engineId) {
  const query = encodeURIComponent(`${keyword} ä¼šç¤¾ ä¼æ¥­ -wikipedia -facebook -twitter -linkedin`);
  const url = `https://www.googleapis.com/customsearch/v1?key=${searchKey}&cx=${engineId}&q=${query}&num=15`;
  
  const response = UrlFetchApp.fetch(url, { method: 'GET' });
  
  if (response.getResponseCode() !== 200) {
    throw new Error(`Google Search API Error: ${response.getResponseCode()}`);
  }
  
  return JSON.parse(response.getContentText());
}

/**
 * ã‚·ãƒ³ãƒ—ãƒ«ãªæ¤œç´¢çµæœè§£æï¼ˆPhase 2: PDFãƒ»æ”¿åºœã‚µã‚¤ãƒˆé™¤å¤–å¼·åŒ–ï¼‰
 */
function analyzeSearchResultSimple(item, keyword) {
  // Phase 2: çµæœé™¤å¤–åˆ¤å®š
  if (shouldExcludeResult(item.title, item.snippet, item.link)) {
    return null;
  }
  
  // åŸºæœ¬çš„ãªä¼æ¥­åæŠ½å‡ºï¼ˆæ”¹è‰¯ç‰ˆ + Phase 3å¯¾å¿œï¼‰
  let companyName = extractCompanyNameEnhanced(item.title, item.snippet, item.link);
  
  // ç„¡åŠ¹ãªãƒ‘ã‚¿ãƒ¼ãƒ³ã‚’é™¤å¤–
  if (!companyName || isInvalidCompanyName(companyName)) {
    return null;
  }
  
  // æ¥­ç•Œæ¨å®š
  const industry = estimateIndustry(item.snippet, item.title);
  
  // åŸºæœ¬ã‚¹ã‚³ã‚¢è¨ˆç®—
  let score = 25; // åŸºæº–ç‚¹ã‚’å¤§å¹…ç·©å’Œï¼ˆä¼æ¥­ç™ºè¦‹æ•°å„ªå…ˆï¼‰
  
  // ã‚¹ã‚³ã‚¢ãƒªãƒ³ã‚°è¦ç´ 
  if (item.title.includes('æ ªå¼ä¼šç¤¾') || item.title.includes('æœ‰é™ä¼šç¤¾')) score += 15;
  if (item.snippet.includes('ä¼šç¤¾æ¦‚è¦') || item.snippet.includes('ä¼æ¥­æƒ…å ±')) score += 10;
  if (item.link.includes('.co.jp')) score += 10;
  if (item.snippet.includes(keyword)) score += 5;
  
  // äº‹æ¥­è€…åŒºåˆ†åˆ¤å®š
  const businessType = determineBusinessType(item.title, item.snippet);
  
  // Phase 4: å€‹äººäº‹æ¥­ä¸»ã®è©³ç´°æŠ½å‡º
  let tradeName = '';
  let representative = '';
  let confidence = score >= 70 ? 'High' : score >= 25 ? 'Medium' : 'Low'; // é–¾å€¤ã‚’25ã«èª¿æ•´
  
  if (businessType === 'å€‹äººäº‹æ¥­ä¸»') {
    const details = extractIndividualBusinessDetails(item.title, item.snippet, item.link);
    tradeName = details.tradeName;
    representative = details.representative;
    confidence = details.confidence > 0 ? 'High' : confidence;
    score += details.confidence; // è©³ç´°æƒ…å ±ãŒã‚ã‚‹ã¨ä¿¡é ¼åº¦ã‚¢ãƒƒãƒ—
  }
  
  return {
    name: companyName,
    website: item.link,
    industry: industry,
    size: estimateCompanySize(item.title, item.snippet, item.link), // å¼·åŒ–ã•ã‚ŒãŸä¼æ¥­è¦æ¨¡åˆ¤å®š
    location: extractLocation(item.snippet),
    description: cleanDescription(item.snippet),
    score: Math.min(score, 100),
    keyword: keyword,
    businessType: businessType,
    tradeName: tradeName,
    representative: representative,
    confidence: confidence
  };
}

