    }
  }
  
  return result;
}

/**
 * ä¼æ¥­åæŠ½å‡ºï¼ˆæ”¹è‰¯ç‰ˆ + Phase 3: æ³•äººæ ¼è£œå®Œå‡¦ç†ï¼‰
 */
function extractCompanyNameEnhanced(title, snippet = '', url = '') {
  // è¨˜äº‹ã‚¿ã‚¤ãƒˆãƒ«ãƒ‘ã‚¿ãƒ¼ãƒ³ã‚’é™¤å¤–
  const articlePatterns = [
    /ãƒ‹ãƒ¥ãƒ¼ã‚¹|è¨˜äº‹|ãƒ—ãƒ¬ã‚¹|ç™ºè¡¨|ã‚¤ãƒ³ã‚¿ãƒ“ãƒ¥ãƒ¼|å–æ|ãƒ¬ãƒãƒ¼ãƒˆ/,
    /ã«ã¤ã„ã¦|ã¨ã„ã†|ã¸ã®|ã§ã®|ã«ã¯|ã‹ã‚‰|ã¾ã§/,
    /\d{4}å¹´\d{1,2}æœˆ|\d{1,2}\/\d{1,2}/,
    /^(ã€|ã€Œ|ã€)/,
    /(ã€‘|ã€|ã€)$/
  ];
  
  if (articlePatterns.some(pattern => pattern.test(title))) {
    return null;
  }
  
  // ä¼æ¥­åãƒ‘ã‚¿ãƒ¼ãƒ³æŠ½å‡º
  const patterns = [
    /([^\s\-|]+(?:æ ªå¼ä¼šç¤¾|æœ‰é™ä¼šç¤¾|åˆåŒä¼šç¤¾|åˆè³‡ä¼šç¤¾|åˆåä¼šç¤¾))/,
    /(æ ªå¼ä¼šç¤¾|æœ‰é™ä¼šç¤¾|åˆåŒä¼šç¤¾|åˆè³‡ä¼šç¤¾|åˆåä¼šç¤¾)[^\s\-|]+/,
    /([^\s\-|]+(?:å…¬ç›Šè²¡å›£æ³•äºº|ä¸€èˆ¬è²¡å›£æ³•äºº|ç¤¾ä¼šç¦ç¥‰æ³•äºº))/,
    /(å…¬ç›Šè²¡å›£æ³•äºº|ä¸€èˆ¬è²¡å›£æ³•äºº|ç¤¾ä¼šç¦ç¥‰æ³•äºº)[^\s\-|]+/,
    /^([^|\-\s]+)/
  ];
  
  let extractedName = null;
  
  for (const pattern of patterns) {
    const match = title.match(pattern);
    if (match && match[1]) {
      extractedName = match[1].trim();
      break;
    }
  }
  
  if (!extractedName) {
    extractedName = title.split(/[\-|]/, 1)[0].trim();
  }
  
  // Phase 3: æ³•äººæ ¼è£œå®Œå‡¦ç†
  return completeCompanyName(extractedName, title, snippet, url);
}

/**
 * Phase 3: ä¸å®Œå…¨ãªä¼æ¥­åã®è£œå®Œ
 */
function completeCompanyName(extractedName, title, snippet, url) {
  // æ³•äººæ ¼ã®ã¿ã®å ´åˆã®å‡¦ç†
  const incompletePatterns = /^(æ ªå¼ä¼šç¤¾|æœ‰é™ä¼šç¤¾|å…¬ç›Šè²¡å›£æ³•äºº|ä¸€èˆ¬è²¡å›£æ³•äºº|åˆåŒä¼šç¤¾|åˆè³‡ä¼šç¤¾|åˆåä¼šç¤¾|ç¤¾ä¼šç¦ç¥‰æ³•äºº)$/i;
  
  if (incompletePatterns.test(extractedName)) {
    // ã‚¿ã‚¤ãƒˆãƒ«ã‚„èª¬æ˜æ–‡ã‹ã‚‰å®Ÿéš›ã®ä¼æ¥­åã‚’æ¤œç´¢
    const fullText = title + ' ' + snippet;
    const namePatterns = [
      /([^\s]{2,20})(æ ªå¼ä¼šç¤¾|æœ‰é™ä¼šç¤¾|å…¬ç›Šè²¡å›£æ³•äºº|ä¸€èˆ¬è²¡å›£æ³•äºº)/g,
      /(æ ªå¼ä¼šç¤¾|æœ‰é™ä¼šç¤¾|å…¬ç›Šè²¡å›£æ³•äºº|ä¸€èˆ¬è²¡å›£æ³•äºº)([^\s]{2,20})/g,
      /([^\s]{3,25})(åˆåŒä¼šç¤¾|åˆè³‡ä¼šç¤¾|åˆåä¼šç¤¾|ç¤¾ä¼šç¦ç¥‰æ³•äºº)/g
    ];
    
    for (const pattern of namePatterns) {
      const matches = [...fullText.matchAll(pattern)];
      for (const match of matches) {
        const fullName = match[0];
        if (fullName !== extractedName && fullName.length > extractedName.length) {
          return fullName.trim();
        }
      }
    }
    
    // URLã‹ã‚‰ä¼æ¥­åã‚’æ¨å®š
    if (url) {
      const domainMatch = url.match(/https?:\/\/(?:www\.)?([^.\/]+)\./);
      if (domainMatch && domainMatch[1]) {
        const domainName = domainMatch[1];
