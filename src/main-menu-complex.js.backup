function shouldExcludeResult(title, snippet, url) {
  // æœ€é‡è¦ãªé™¤å¤–ã®ã¿ï¼ˆè³ªã¨é‡ã®ãƒãƒ©ãƒ³ã‚¹é‡è¦–ï¼‰
  const criticalExclusions = [
